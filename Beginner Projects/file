for(int i = 0; i < 5; i++) {
System.out.println("Text appears but may go over " +
"multiple lines!"); // line was long
}
if (the_first == 0 && the_second == 0 &&
the_third == 0 && the_fourth == 0 &&
the_fifth == 0 && the_sixth == 0) {
System.out.println("Text appears but may go over " +
"multiple lines!"); // line was long
}
}
}